urn {\n                openIndex: -1,\n                opt: null,\n                width: 1000,\n                height: 1000\n            }\n        },\n        created(){\n            if(typeof  this.images !== typeof []){\n                console.error(\"[Gallery] images prop must be an array\")\n            }\n        },\n        mounted() {\n            this.width = this.$el.getBoundingClientRect().width;\n            this.height = this.$el.getBoundingClientRect().height;\n            console.log(this.width)\n        }\n    }\n</script>"]}]}